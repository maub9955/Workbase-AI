{
  "name": "notion-clone",
  "version": "0.1.0",
  "private": true,
  "description": "Internal Notion-style collaboration suite",
  "type": "module",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "prepare": "husky"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "keywords": [
    "notion",
    "collaboration",
    "workspace"
  ],
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  },
  "dependencies": {
    "@tiptap/extension-placeholder": "^3.10.7",
    "@tiptap/react": "^3.10.7",
    "@tiptap/starter-kit": "^3.10.7",
    "zod": "^4.1.12"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs,cjs}": "eslint --fix",
    "*.{md,json,yml,yaml}": "prettier --write"
  },
  "packageManager": "npm@11.3.0"
}
